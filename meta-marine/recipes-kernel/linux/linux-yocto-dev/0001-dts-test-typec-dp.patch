From d8729a7b978f20b2a4f27c452215ec2d325d674d Mon Sep 17 00:00:00 2001
From: Trinh Viet Quang <vietquang0708@gmail.com>
Date: Wed, 3 Sep 2025 01:52:21 +0700
Subject: [PATCH] dts: test typec dp

Upstream-Status: Inappropriate [oe specific]

Signed-off-by: Trinh Viet Quang <vietquang0708@gmail.com>
---
 arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi | 10 +++-------
 1 file changed, 3 insertions(+), 7 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi b/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
index 13fa545456e1..e9b3553449ed 100644
--- a/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
@@ -520,14 +520,11 @@ connector {
 			compatible = "usb-c-connector";
 			data-role = "dual";
 			label = "USB-C";
-			op-sink-microwatt = <1000000>;
-			/* fusb302 supports PD Rev 2.0 Ver 1.2 */
-			pd-revision = /bits/ 8 <0x2 0x0 0x1 0x2>;
 			power-role = "dual";
-			sink-pdos = <PDO_FIXED(5000, 3000, PDO_FIXED_USB_COMM)>;
-			source-pdos = <PDO_FIXED(5000, 2000, PDO_FIXED_USB_COMM)>;
+			op-sink-microwatt = <10>;
+			source-pdos = <PDO_FIXED(5000, 1400, PDO_FIXED_USB_COMM)>;
+			sink-pdos = <PDO_FIXED(5000, 10, PDO_FIXED_USB_COMM)>;
 			try-power-role = "source";
-			typec-power-opmode = "1.5A";
 
 			ports {
 				#address-cells = <1>;
@@ -1278,7 +1275,6 @@ &usb_host0_ohci {
 &usb_host0_xhci {
 	usb-role-switch;
 	status = "okay";
-	dr_mode = "host";
 	port {
 		usb_host0_xhci_drd_sw: endpoint {
 			remote-endpoint = <&usbc0_hs>;
-- 
