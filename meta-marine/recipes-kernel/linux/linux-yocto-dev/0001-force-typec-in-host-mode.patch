From 7aff180c7df79362805ed0f0ff7b6925a143a969 Mon Sep 17 00:00:00 2001
From: Trinh Viet Quang <vietquang0708@gmail.com>
Date: Wed, 3 Sep 2025 21:16:04 +0700
Subject: [PATCH] force typec in host mode

Upstream-Status: Inappropriate [oe specific]

Signed-off-by: Trinh Viet Quang <vietquang0708@gmail.com>
---
 arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi b/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
index 4ea8ffafb348..d4b21b182d9d 100644
--- a/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
@@ -539,7 +539,7 @@ usbc0: usb-typec@22 {
 		pinctrl-names = "default";
 		pinctrl-0 = <&usbc0_int>;
 		vbus-supply = <&vbus5v0_typec>;
-		status = "okay";
+		status = "disabled";
 
 		connector {
 			compatible = "usb-c-connector";
@@ -1302,13 +1302,13 @@ &usb_host1_ehci {
 };
 
 &usb_host1_ohci {
-	dr_mode = "host";
+	// dr_mode = "host";
 	status = "okay";
 };
 
-&usb_host2_xhci {
-	status = "okay";
-};
+// &usb_host2_xhci {
+// 	status = "okay";
+// };
 
 &vop_mmu {
 	status = "okay";
