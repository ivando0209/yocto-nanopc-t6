From 42cf6610538151b65a65294dbd3cd005aef34dec Mon Sep 17 00:00:00 2001
From: Trinh Viet Quang <trinh.quang@cavliwireless.com>
Date: Mon, 25 Aug 2025 02:14:28 +0700
Subject: [PATCH 11/13] gmac1: reset active low

Upstream-Status: Inappropriate [oe specific]

Signed-off-by: Trinh Viet Quang <trinh.quang@cavliwireless.com>
---
 arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi b/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
index 04e4836483c9..37cde87845eb 100644
--- a/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3588-nanopc-t6.dtsi
@@ -656,7 +656,7 @@ &pcie2x1l0 {
 	// control the power enable for lane 0
 	// - Lane 0 is used for M.2 2.5G Ethernet
 	//===============================================
-	reset-gpios = <&gpio4 RK_PB3 GPIO_ACTIVE_HIGH>;
+	reset-gpios = <&gpio4 RK_PB3 GPIO_ACTIVE_LOW>;
 	vpcie3v3-supply = <&vcc_3v3_pcie20>;
 	pinctrl-names = "default";
 	pinctrl-0 = <&pcie2_0_rst>;
